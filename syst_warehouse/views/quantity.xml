<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="quantity_warehouse_view_tree" model="ir.ui.view">
            <field name="name">quantity.warehouse.view.tree</field>
            <field name="model">quantity.warehouse</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Quanty">
                    <field name="sequence" widget="handle"/>
                    <field name="name" required="1"/>
                    <field name="code" />
                </tree>
            </field>
        </record>

        <record id="quantity_warehouse_action" model="ir.actions.act_window">
            <field name="name">Quantity</field>
            <field name="res_model">quantity.warehouse</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
    

</odoo>
